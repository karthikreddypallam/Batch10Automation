<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Smoke Suite">
	<groups>
	<run>
		<include name="sanity"/>
		<exclude name="smoke"/>
	</run>
	</groups>
	<parameter name="browser" value="firefox"/>
	<parameter name="env" value="beta"/>
	<test name = "Login">
	<classes>
		<class name = "Batch10.Batch10MvnAutomationProj.Login"/>
		<class name = "Batch10.Batch10MvnAutomationProj.Register"/>
	</classes>
	</test>
</suite>